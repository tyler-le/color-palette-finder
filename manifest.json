{
  "manifest_version": 2,
  "name": "Color Palette Picker",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "webRequest", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "background": {
    "scripts": ["popup.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
